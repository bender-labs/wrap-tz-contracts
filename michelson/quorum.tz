                 CDR ;
                 SWAP ;
                 CAR ;
                 DIG 2 ;
                 DUP ;
                 DUG 3 ;
                 CDR ;
                 CAR ;
                 PAIR ;
                 DIG 2 ;
                 CAR ;
                 PAIR ;
                 DUP ;
                 CDR ;
                 CDR ;
                 DIG 2 ;
                 PAIR ;
                 SWAP ;
                 CAR ;
                 PAIR }
               { SWAP ; DUP ; DUG 2 ; CDR ; CAR ; PAIR ; SWAP ; CAR ; PAIR } ;
             NIL operation ;
             PAIR }
           { SWAP ;
             DUP ;
             DIG 2 ;
             SWAP ;
             DUP ;
             DUG 2 ;
             CDR ;
             CDR ;
             SWAP ;
             DUP ;
             DUG 2 ;
             CDR ;
             SIZE ;
             COMPARE ;
             LT ;
             IF { PUSH string "MISSING_SIGNATURES" ; FAILWITH } {} ;
             DUP ;
             CAR ;
             SELF ;
             ADDRESS ;
             CHAIN_ID ;
             PAIR ;
             PAIR ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             CDR ;
             CAR ;
             DIG 3 ;
             CDR ;
             CDR ;
             PAIR ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             CDR ;
             DIG 2 ;
             PACK ;
             PAIR ;
             PAIR ;
             PUSH nat 0 ;
             SWAP ;
             DUP ;
             DUG 2 ;
             CAR ;
             CDR ;
             ITER { DIG 2 ;
                    DUP ;
                    DUG 3 ;
                    CDR ;
                    CDR ;
                    SWAP ;
                    DUP ;
                    DUG 2 ;
                    CAR ;
                    GET ;
                    IF_NONE { PUSH string "SIGNER_UNKNOWN" ; FAILWITH } {} ;
                    DIG 3 ;
                    DUP ;
                    DUG 4 ;
                    CAR ;
                    CAR ;
                    DIG 2 ;
                    CDR ;
                    DIG 2 ;
                    CHECK_SIGNATURE ;
                    IF { PUSH nat 1 ; ADD } {} } ;
             SWAP ;
             CDR ;
             CAR ;
             SWAP ;
             COMPARE ;
             LT ;
             IF { PUSH string "BAD_SIGNATURE" ; FAILWITH } {} ;
             CAR ;
             DUP ;
             CDR ;
             CONTRACT
               (or (or (pair %add_fungible_token (bytes %eth_contract) (pair %token_address address nat))
                       (pair %add_nft (bytes %eth_contract) (address %token_contract)))
                   (or (pair %mint_fungible_token
                          (bytes %erc_20)
                          (pair (pair %event_id (bytes %block_hash) (nat %log_index))
                                (pair (address %owner) (nat %amount))))
                       (pair %mint_nft
                          (bytes %erc_721)
                          (pair (pair %event_id (bytes %block_hash) (nat %log_index))
                                (pair (address %owner) (nat %token_id)))))) ;
             IF_NONE { PUSH string "BAD_CONTRACT_TARGET" ; FAILWITH } {} ;
             NIL operation ;
             SWAP ;
             PUSH mutez 0 ;
